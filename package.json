{
  "name": "list-maker-8",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "include-all": "^1.0.0",
    "is-browser": "^2.0.1",
    "node-sass": "^4.5.3",
    "rc": "1.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "sails": "~0.12.13",
    "sails-disk": "~0.10.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.11.0",
    "react-scripts": "1.0.7"
  },
  "proxy": "http://localhost:1337",
  "scripts": {
    "styles:build": "node_modules/.bin/node-sass --output src/styles src/styles.scss",
    "styles:watch": "node_modules/.bin/node-sass --watch --output src/styles src/styles.scss",
    "dev": "npm run styles:build && npm run styles:watch & npm run dev:server & npm run dev:client",
    "dev:styles": "react-scripts start",
    "dev:client": "react-scripts start",
    "dev:server": "node_modules/.bin/nodemon node_modules/.bin/babel-node app.js",
    "prod": "npm run prod:client && npm run prod:server && npm run prod:launch",
    "prod:styles": "node_modules/.bin/node-sass --output assets/styles src/scss/styles.scss",
    "prod:client": "node_modules/.bin/react-scripts build && rm -rf assets/build && mv build assets",
    "prod:server": "node_modules/.bin/babel -d react src/**/*.js",
    "prod:launch": "NODE_ENV=production node app",
    "test": "react-scrwipts test --env=jsdom"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/hovanem/list-maker-8.git"
  },
  "author": "hovanem",
  "license": ""
}
